groups:
- name: HighRequestLatency
  rules:
  - alert: high_latency
    expr: sum(rate(http_requests_duration_ms_bucket{le="0.25"}[10s])) / sum(rate(http_requests_duration_ms_count[10s])) < 0.90
    for: 5s
    labels:
      severity: warning
    annotations:
      summary: "Server "
      description: "Latency is high"
